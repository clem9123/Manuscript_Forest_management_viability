@article{silvapedroDisentanglingEffectsCompositional2017,
  title = {Disentangling the Effects of Compositional and Structural Diversity on Forest Productivity},
  author = {Silva Pedro, Mariana and Rammer, Werner and Seidl, Rupert},
  date = {2017},
  journaltitle = {Journal of Vegetation Science},
  volume = {28},
  number = {3},
