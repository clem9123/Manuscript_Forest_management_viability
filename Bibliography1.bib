  author = {Schall, Peter and Gossner, Martin M. and Heinrichs, Steffi and Fischer, Markus and Boch, Steffen and Prati, Daniel and Jung, Kirsten and Baumgartner, Vanessa and Blaser, Stefan and Böhm, Stefan and Buscot, François and Daniel, Rolf and Goldmann, Kezia and Kaiser, Kristin and Kahl, Tiemo and Lange, Markus and Müller, Jörg and Overmann, Jörg and Renner, Swen C. and Schulze, Ernst-Detlef and Sikorski, Johannes and Tschapka, Marco and Türke, Manfred and Weisser, Wolfgang W. and Wemheuer, Bernd and Wubet, Tesfaye and Ammer, Christian},
