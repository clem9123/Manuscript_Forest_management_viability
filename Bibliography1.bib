@article{imaiEffectsSelectiveLogging2012,
  title = {Effects of Selective Logging on Tree Species Diversity and Composition of {{Bornean}} Tropical Rain Forests at Different Spatial Scales},
  author = {Imai, Nobuo and Seino, Tatsuyuki and Aiba, Shin-ichiro and Takyu, Masaaki and Titin, Jupiri and Kitayama, Kanehiro},
  date = {2012-09-01},
  journaltitle = {Plant Ecology},
  shortjournal = {Plant Ecol},
  volume = {213},
  number = {9},
